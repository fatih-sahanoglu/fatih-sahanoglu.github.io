{
	"presets": [
		[
			"@babel/stage-2",
			{
				"decoratorsLegacy": true
			}
		],
		"@babel/react",
		[
			"@babel/env",
			{
				"production": {
					"presets": ["minify"]
				},
				"targets": {
					"browsers": [
						"last 2 chrome versions",
						"last 2 opera versions",
						"last 2 firefox versions",
						"last 2 ios versions",
						"last 2 chromeandroid versions",
						"last 2 edge versions"
					]
				}
			}
		]
	],
	"plugins": [["babel-plugin-styled-components", {
		"displayName": false
	}]]
}
