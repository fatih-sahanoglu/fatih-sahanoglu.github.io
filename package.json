{
	"name": "fatih-sahanoglu.github.io",
	"version": "1.0.0",
	"description": "Homepage of Fatih Sahanoglu",
	"main": "index.js",
	"repository": "git@github.com:fatih-sahanoglu/fatih-sahanoglu.github.io.git",
	"author": "Gregor Adams <greg@pixelass.com>",
	"license": "MIT",
	"scripts": {
		"babel": "babel src --out-dir lib",
		"build": "cross-env NODE_ENV=production webpack",
		"prebuild": "yarn clean",
		"clean": "del about care contact service impressum images icons iconstats.json '*.html' '*.js' '!*.config.js' '!.*rc.js'",
		"dev": "webpack-dev-server --hot",
		"format": "yarn format:md; yarn format:js; yarn format:json",
		"format:json": "prettier package.json .*rc 'src/**/*.json' --parser json --write",
		"format:js": "prettier *.config.js .*rc.js '{scripts,src}/**/*.js' --parser babylon --write",
		"format:md": "prettier '*.md' 'src/**/*.md' --parser markdown --write",
		"start": "yarn build && http-server",
		"test": "ava"
	},
	"browserslist": [
		"last 2 chrome versions",
		"last 2 opera versions",
		"last 2 firefox versions",
		"last 2 ios versions",
		"last 2 chromeandroid versions",
		"last 2 edge versions"
	],
	"dependencies": {
		"markdown-react-js": "^1.0.2",
		"prop-types": "^15.7.2",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-router-dom": "^5.1.2",
		"react-scroll": "^1.7.14",
		"react-spring": "^8.0.27",
		"styled-components": "^4.4.0"
	},
	"devDependencies": {
		"@babel/cli": "7.6.4",
		"@babel/core": "7.6.4",
		"@babel/preset-env": "7.6.3",
		"@babel/preset-react": "7.6.3",
		"@babel/preset-stage-2": "7.0.0",
		"ava": "2.4.0",
		"babel-loader": "8.0.6",
		"babel-minify-webpack-plugin": "0.3.1",
		"babel-plugin-styled-components": "1.10.6",
		"cross-env": "6.0.3",
		"del-cli": "3.0.0",
		"favicons-webpack-plugin": "1.0.2",
		"file-loader": "4.2.0",
		"globby": "11.0.1",
		"html-webpack-harddisk-plugin": "1.0.1",
		"html-webpack-plugin": "3.2.0",
		"http-server": "0.11.1",
		"imagemin-mozjpeg": "8.0.0",
		"imagemin-webp": "5.1.0",
		"imagemin-webpack-plugin": "2.4.2",
		"json-loader": "0.5.7",
		"mkdirp": "0.5.1",
		"pify": "4.0.1",
		"pug": "2.0.4",
		"pug-loader": "2.4.0",
		"sharp": "0.23.1",
		"sharp-loader": "1.1.0",
		"webpack": "4.41.0",
		"webpack-cli": "3.3.9",
		"webpack-dev-server": "3.8.2",
		"yaml-frontmatter-loader": "0.1.0"
	}
}
